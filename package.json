  },
  "engines": {
    "node": ">=12.13.x",
    "yarn": ">=1.19.x"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|less|md|yml)": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
